package claseTarea;

class TeoremaCuadrado
{
	double numero1;

	double numero2;

	double numero3;


		void establecerNumero1(double a)
		{
			numero1 = a;
		}

		double obtenerNumero1()
		{
			return numero1;
		}
		void establecerNumero2(double b)
		{
			numero2 = b;
		}

		double obtenerNumero2()
		{
			return numero2;			
		}
		void establecerNumero3(double c)
		{
			numero3 = c;
		}

		double obtenerNumero3()
		{
			return numero3;			
		}


		void opeCuadraticaN()
		{
			double opeN;
			opeN = (-obtenerNumero2() - Math.sqrt(Math.pow(obtenerNumero2(),2)-4*obtenerNumero1()*obtenerNumero3()))/2*obtenerNumero1();

			return opeN;
		}

		void opeCuadraticaP()
		{
			double opeP;
			opeP = (-obtenerNumero2() - Math.sqrt(Math.pow(obtenerNumero2(),2)-4*obtenerNumero1()*obtenerNumero3()))/2*obtenerNumero1();

			return opeP;
		}

}
